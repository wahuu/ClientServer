package pl.server.pojo;

import pl.server.dto.MediaLibrary;

public class ExtraSchedule {

	private MediaLibrary media;
	private String ip;

	public ExtraSchedule(MediaLibrary media, String ip) {
		super();
		this.media = media;
		this.ip = ip;
	}

	public MediaLibrary getMedia() {
		return media;
	}

	public void setMedia(MediaLibrary media) {
		this.media = media;
	}

	public String getIp() {
		return ip;
	}

	public void setIp(String ip) {
		this.ip = ip;
	}

}
