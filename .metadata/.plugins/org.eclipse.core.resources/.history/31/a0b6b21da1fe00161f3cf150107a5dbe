package pl.server.pojo;

public class ExtraSchedule {

	private String media;
	private String ip;

	public ExtraSchedule(String media, String ip) {
		this.media = media;
		this.ip = ip;
	}

	public String getMedia() {
		return media;
	}

	public void setMedia(String media) {
		this.media = media;
	}

	public String getIp() {
		return ip;
	}

	public void setIp(String ip) {
		this.ip = ip;
	}

}
